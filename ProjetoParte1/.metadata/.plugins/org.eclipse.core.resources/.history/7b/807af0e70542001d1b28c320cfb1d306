package sintaxeVariaveis_e_Fluxo;

public class DesafioFibonacci {
	
	public static void main (String [] args) {
		
		int proximo = 0;
		int anterior = 1;
		int resultado = 0;
		for (int contador = 0 ; contador <= 5; contador ++) {
			
			resultado = proximo + anterior;
			System.out.println(resultado);
			proximo = resultado;
			anterior = proximo;
			
			
		}
	}

}
